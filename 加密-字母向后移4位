#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
int main()
{
    char arr[6];
    scanf("%s", arr);
    int i;
    for (i = 0; i < 5; i++)
    {
        if (arr[i] >= 'A' && arr[i] <= 'Z')
            arr[i] = (arr[i] - 'A' + 4) % 26 + 'A';
        if (arr[i] >= 'a' && arr[i] <= 'z')
            arr[i] = (arr[i] - 'a' + 4) % 26 + 'a';
    }
    printf("%s", arr);
    return 0;
}
